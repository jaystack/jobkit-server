{
  "name": "jobkits",
  "version": "0.0.1",
  "description": "JavaScript job runner",
  "main": "lib/server/index.js",
  "scripts": {
    "tsc": "tsc -p src/server --outDir lib/server",
    "tsc:w": "tsc -p src/server --outDir lib/server -w",
    "build": "rm -rf lib && npm run tsc",
    "build:w": "npm run tsc:w",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaystack/jobkits.git"
  },
  "keywords": [
    "job",
    "runner",
    "task",
    "build"
  ],
  "author": "JayStack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaystack/jobkits/issues"
  },
  "homepage": "https://github.com/jaystack/jobkits#readme",
  "devDependencies": {
    "@types/config": "0.0.32",
    "@types/express": "^4.0.36",
    "@types/fs-extra": "^3.0.3",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "config": "^1.26.1",
    "express": "^4.15.3",
    "fs-extra": "^4.0.0",
    "simple-git": "^1.73.0"
  }
}
